	/**
	 * @generated
	 */
	public final boolean provides(IOperation operation) {
		if (operation instanceof CreateViewForKindOperation) {
			return provides((CreateViewForKindOperation) operation);
		}
		assert operation instanceof CreateViewOperation;
		if (operation instanceof CreateDiagramViewOperation) {
			return provides((CreateDiagramViewOperation) operation);
		} else if (operation instanceof CreateEdgeViewOperation) {
			return provides((CreateEdgeViewOperation) operation);
		} else if (operation instanceof CreateNodeViewOperation) {
			return provides((CreateNodeViewOperation) operation);
		}
		return false;
	}

	/**
	 * @generated
	 */
	protected boolean provides(CreateViewForKindOperation op) {
		/*
		 if (op.getViewKind() == Node.class)
		 return getNodeViewClass(op.getSemanticAdapter(), op.getContainerView(), op.getSemanticHint()) != null;
		 if (op.getViewKind() == Edge.class)
		 return getEdgeViewClass(op.getSemanticAdapter(), op.getContainerView(), op.getSemanticHint()) != null;
		 */
		return true;
	}

	/**
	 * @generated
	 */
	protected boolean provides(CreateDiagramViewOperation op) {
		return AreaDesenhoEditPart.MODEL_ID.equals(op.getSemanticHint())
				&& DsmlVisualIDRegistry
						.getDiagramVisualID(getSemanticElement(op
								.getSemanticAdapter())) != -1;
	}

	/**
	 * @generated
	 */
	protected boolean provides(CreateNodeViewOperation op) {
		if (op.getContainerView() == null) {
			return false;
		}
		IElementType elementType = getSemanticElementType(op
				.getSemanticAdapter());
		EObject domainElement = getSemanticElement(op.getSemanticAdapter());
		int visualID;
		if (op.getSemanticHint() == null) {
			// Semantic hint is not specified. Can be a result of call from CanonicalEditPolicy.
			// In this situation there should be NO elementType, visualID will be determined
			// by VisualIDRegistry.getNodeVisualID() for domainElement.
			if (elementType != null || domainElement == null) {
				return false;
			}
			visualID = DsmlVisualIDRegistry.getNodeVisualID(
					op.getContainerView(), domainElement);
		} else {
			visualID = DsmlVisualIDRegistry.getVisualID(op.getSemanticHint());
			if (elementType != null) {
				if (!DsmlElementTypes.isKnownElementType(elementType)
						|| (!(elementType instanceof IHintedType))) {
					return false; // foreign element type
				}
				String elementTypeHint = ((IHintedType) elementType)
						.getSemanticHint();
				if (!op.getSemanticHint().equals(elementTypeHint)) {
					return false; // if semantic hint is specified it should be the same as in element type
				}
				if (domainElement != null
						&& visualID != DsmlVisualIDRegistry.getNodeVisualID(
								op.getContainerView(), domainElement)) {
					return false; // visual id for node EClass should match visual id from element type
				}
			} else {
				if (!AreaDesenhoEditPart.MODEL_ID.equals(DsmlVisualIDRegistry
						.getModelID(op.getContainerView()))) {
					return false; // foreign diagram
				}
				switch (visualID) {
				case AggregationEditPart.VISUAL_ID:
				case OrderEditPart.VISUAL_ID:
				case OperatorEditPart.VISUAL_ID:
				case ValueOperatorEditPart.VISUAL_ID:
				case UnaryOperatorPatternEditPart.VISUAL_ID:
				case BinaryOperatorPatternEditPart.VISUAL_ID:
				case PatternTimerEditPart.VISUAL_ID:
				case ConditionGroupEditPart.VISUAL_ID:
				case TargetEditPart.VISUAL_ID:
				case NotEditPart.VISUAL_ID:
				case RuleEditPart.VISUAL_ID:
				case EventPatternEditPart.VISUAL_ID:
				case WindowEditPart.VISUAL_ID:
				case EventEditPart.VISUAL_ID:
				case LimitEditPart.VISUAL_ID:
				case AttributeEditPart.VISUAL_ID:
				case Operator2EditPart.VISUAL_ID:
				case ValueOperator2EditPart.VISUAL_ID:
				case Operator3EditPart.VISUAL_ID:
				case ValueOperator3EditPart.VISUAL_ID:
				case Aggregation2EditPart.VISUAL_ID:
				case Order2EditPart.VISUAL_ID:
				case Operator4EditPart.VISUAL_ID:
				case ValueOperator4EditPart.VISUAL_ID:
				case UnaryOperatorPattern2EditPart.VISUAL_ID:
				case BinaryOperatorPattern2EditPart.VISUAL_ID:
				case PatternTimer2EditPart.VISUAL_ID:
				case ConditionGroup2EditPart.VISUAL_ID:
				case Target2EditPart.VISUAL_ID:
				case Not2EditPart.VISUAL_ID:
				case Aggregation3EditPart.VISUAL_ID:
				case Order3EditPart.VISUAL_ID:
				case Operator5EditPart.VISUAL_ID:
				case ValueOperator5EditPart.VISUAL_ID:
				case UnaryOperatorPattern3EditPart.VISUAL_ID:
				case BinaryOperatorPattern3EditPart.VISUAL_ID:
				case PatternTimer3EditPart.VISUAL_ID:
				case ConditionGroup3EditPart.VISUAL_ID:
				case Target3EditPart.VISUAL_ID:
				case Not3EditPart.VISUAL_ID:
					if (domainElement == null
							|| visualID != DsmlVisualIDRegistry
									.getNodeVisualID(op.getContainerView(),
											domainElement)) {
						return false; // visual id in semantic hint should match visual id for domain element
					}
					break;
				default:
					return false;
				}
			}
		}
		return AggregationEditPart.VISUAL_ID == visualID
				|| OrderEditPart.VISUAL_ID == visualID
				|| OperatorEditPart.VISUAL_ID == visualID
				|| ValueOperatorEditPart.VISUAL_ID == visualID
				|| UnaryOperatorPatternEditPart.VISUAL_ID == visualID
				|| BinaryOperatorPatternEditPart.VISUAL_ID == visualID
				|| PatternTimerEditPart.VISUAL_ID == visualID
				|| ConditionGroupEditPart.VISUAL_ID == visualID
				|| TargetEditPart.VISUAL_ID == visualID
				|| NotEditPart.VISUAL_ID == visualID
				|| RuleEditPart.VISUAL_ID == visualID
				|| EventPatternEditPart.VISUAL_ID == visualID
				|| WindowEditPart.VISUAL_ID == visualID
				|| EventEditPart.VISUAL_ID == visualID
				|| LimitEditPart.VISUAL_ID == visualID
				|| Operator2EditPart.VISUAL_ID == visualID
				|| ValueOperator2EditPart.VISUAL_ID == visualID
				|| Operator3EditPart.VISUAL_ID == visualID
				|| ValueOperator3EditPart.VISUAL_ID == visualID
				|| Aggregation2EditPart.VISUAL_ID == visualID
				|| Order2EditPart.VISUAL_ID == visualID
				|| Operator4EditPart.VISUAL_ID == visualID
				|| ValueOperator4EditPart.VISUAL_ID == visualID
				|| UnaryOperatorPattern2EditPart.VISUAL_ID == visualID
				|| BinaryOperatorPattern2EditPart.VISUAL_ID == visualID
				|| PatternTimer2EditPart.VISUAL_ID == visualID
				|| ConditionGroup2EditPart.VISUAL_ID == visualID
				|| Target2EditPart.VISUAL_ID == visualID
				|| Not2EditPart.VISUAL_ID == visualID
				|| Aggregation3EditPart.VISUAL_ID == visualID
				|| Order3EditPart.VISUAL_ID == visualID
				|| Operator5EditPart.VISUAL_ID == visualID
				|| ValueOperator5EditPart.VISUAL_ID == visualID
				|| UnaryOperatorPattern3EditPart.VISUAL_ID == visualID
				|| BinaryOperatorPattern3EditPart.VISUAL_ID == visualID
				|| PatternTimer3EditPart.VISUAL_ID == visualID
				|| ConditionGroup3EditPart.VISUAL_ID == visualID
				|| Target3EditPart.VISUAL_ID == visualID
				|| Not3EditPart.VISUAL_ID == visualID
				|| AttributeEditPart.VISUAL_ID == visualID;
	}

	/**
	 * @generated
	 */
	protected boolean provides(CreateEdgeViewOperation op) {
		IElementType elementType = getSemanticElementType(op
				.getSemanticAdapter());
		if (!DsmlElementTypes.isKnownElementType(elementType)
				|| (!(elementType instanceof IHintedType))) {
			return false; // foreign element type
		}
		String elementTypeHint = ((IHintedType) elementType).getSemanticHint();
		if (elementTypeHint == null
				|| (op.getSemanticHint() != null && !elementTypeHint.equals(op
						.getSemanticHint()))) {
			return false; // our hint is visual id and must be specified, and it should be the same as in element type
		}
		int visualID = DsmlVisualIDRegistry.getVisualID(elementTypeHint);
		EObject domainElement = getSemanticElement(op.getSemanticAdapter());
		if (domainElement != null
				&& visualID != DsmlVisualIDRegistry
						.getLinkWithClassVisualID(domainElement)) {
			return false; // visual id for link EClass should match visual id from element type
		}
		return true;
	}

	/**
	 * @generated
	 */
	public Diagram createDiagram(IAdaptable semanticAdapter,
			String diagramKind, PreferencesHint preferencesHint) {
		Diagram diagram = NotationFactory.eINSTANCE.createDiagram();
		diagram.getStyles().add(NotationFactory.eINSTANCE.createDiagramStyle());
		diagram.setType(AreaDesenhoEditPart.MODEL_ID);
		diagram.setElement(getSemanticElement(semanticAdapter));
		diagram.setMeasurementUnit(MeasurementUnit.PIXEL_LITERAL);
		return diagram;
	}

